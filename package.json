{
  "name": "bunny-boilerplate",
  "version": "2.0.0",
  "description": "Personal react/redux boilerplate by Lochlan Bunn.",
  "scripts": {
    "start": "hjs-dev-server",
    "build": "NODE_ENV=production webpack --progress",
    "test": "standard --verbose src/**/*.js | snazzy",
    "deploy": "npm run build && surge --project ./public --domain bunny-boilerplate.surge.sh"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "css-modules",
    "dux",
    "standardjs"
  ],
  "license": "MIT",
  "author": "Lochlan Bunn <bunn@lochlan.io>",
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel": "^6.5.2",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "core-js": "^2.4.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.1",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^2.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "hjs-webpack": "^8.3.0",
    "json-loader": "^0.5.4",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
    "snazzy": "^4.0.0",
    "standard": "^7.1.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack-visualizer-plugin": "^0.1.5"
  },
  "dependencies": {
    "flux-standard-action": "^0.6.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  },
  "standard": {
    "parser": "babel-eslint"
  }
